@{
    ViewBag.Title = "showbookings";
}

<h2>showbookings</h2>

<table>
    <tr>
        <td>ID</td>
        &nbsp;&nbsp;
        <td>Name </td>
        <td>phone </td>
        <td>Time </td>
        <td>Check in  </td>
        <td>check out  </td>
        <td>members  </td>
        <td>Required  </td>
    </tr>

    @foreach (var s in Model)
    {
        <tr>
            <td>@s.Id  </td>
            <td>@s.Name  </td>
            <td>@s.Sur_Name  </td>
            <td>@s.Email  </td>
            <td>@s.Phone  </td>
            <td>@s.Time  </td>
            <td>@s.check_in     </td>
            <td>@s.check_out  </td>
            <td>@s.members  </td>
            <td>@s.Required  </td>


        </tr>
    }
</table>